lappend auto_path [pwd]/../
package require tcltest
package require conf

proc cb-1.0 {_conf _ctx op kname kval} {
	upvar #0 $_conf conf

	switch $op {
	=S -
	=L {
		dict set conf {*}$kname $kval
	}
	+=S {
		if {[dict exists $conf {*}$kname]} {
			set val [dict get $conf {*}$kname]
		} else {
			set val ""
		}
		append val $kval
		dict set conf {*}$kname $val
	}
	+=L {
		if {[dict exists $conf {*}$kname]} {
			set val [dict get $conf {*}$kname]
		} else {
			set val ""
		}
		lappend val {*}$kval
		dict set conf {*}$kname $val
	}
	"SECT_CH" -
	F {
	}
	}
}

tcltest::test str-1.0 {
	opt -s
} -body {
	set ::conf ""
	conf::load_from_str -s 5 {cb-1.0 ::conf} "k1=v1 k2=v2"
	return $::conf
} -result {k2 v2}

